<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Dashboard</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Visitors per day in the last month
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="flot-chart">
          <div class="flot-chart-content" id="flot-line-chart"></div>
        </div>
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>

  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        People by profession
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="flot-chart flot-label-leaning" style="padding-bottom: 40px;">
          <div class="flot-chart-content" id="flot-bar-chart"></div>
        </div>
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>

</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag 'admin/statistics' %>
<% end %>